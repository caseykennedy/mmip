    "@sanity/icons": "^3.7.4",
    "@sanity/vision": "^4.2.0",
    "date-fns": "^3.6.0",
    "pluralize-esm": "^9.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rxjs": "^7.8.1",
    "sanity": "^4.1.1",
    "sanity-plugin-asset-source-unsplash": "^3.0.4",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^4.0.0",
    "@types/react": "^18.3.12",
    "eslint": "^8.6.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 100,
